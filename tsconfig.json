{
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "lib": [ "es2021" ],
    "moduleResolution": "node16",
    "declaration": true,
    "inlineSources": true,
    "newLine": "lf",
    "outDir": "dist",
    "preserveConstEnums": true,
    "sourceMap": true,
    "stripInternal": true,
    "baseUrl": "src",
    "paths": {
      "../../../*": [ "../../node_modules/@solid/community-server/dist/*" ],
      "../../*": [ "../../node_modules/@solid/community-server/dist/*" ],
      "../*": [ "../../node_modules/@solid/community-server/dist/identity/interaction/*" ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}

